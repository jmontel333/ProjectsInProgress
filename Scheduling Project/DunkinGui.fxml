<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Main">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane maxWidth="700.0" minWidth="700.0" prefHeight="450.0" prefWidth="700.0">
         <children>
            <Button fx:id="editButton" mnemonicParsing="false" prefHeight="456.0" prefWidth="350.0" text="Edit the staff and manager list and availabilities" textFill="#673716" wrapText="true">
               <font>
                  <Font size="36.0" />
               </font></Button>
            <Button fx:id="scheduleButton" layoutX="350.0" maxHeight="646.0" maxWidth="350.0" minHeight="450.0" minWidth="350.0" mnemonicParsing="false" prefHeight="456.0" prefWidth="350.0" text="Generate a schedule for the week using the current staff and manager settings" textFill="#673716" wrapText="true">
               <font>
                  <Font size="36.0" />
               </font>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </Button>
         </children>
      </AnchorPane>
   </children>
